FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.9";
    object      gradTz;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    hot
    {
        type            calculated;
        value           uniform 0;
    }
    cold
    {
        type            calculated;
        value           uniform 0;
    }
    interface_left
    {
        type            calculated;
        value           uniform 0;
    }
    interface_right
    {
        type            calculated;
        value           uniform 0;
    }
    interface_top
    {
        type            calculated;
        value           uniform 0;
    }
    interface_bottom
    {
        type            calculated;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


